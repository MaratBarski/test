{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@env/*": [
        "src/environments/*"
      ],
      "@app/core-api": [
        "./projects/core/src/public-api"
      ],
      "@app/*": [
        "src/app/*"
      ],
      "@appcore": [
        "./projects/core/src/public-api"
      ],
      "core": [
        "dist/core"
      ],
      "core/*": [
        "dist/core/*"
      ],
      "@mdc/ngx-charts": [
        "./projects/swimlane/ngx-charts/src/public_api"
      ],
      "@mdc/ngx-charts/*": [
        "./projects/swimlane/ngx-charts/src/lib/*"
      ]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}